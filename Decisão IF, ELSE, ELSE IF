#include <stdio.h>

int main() {
 
int e_velocidade;

printf("Digite a velocidade atual:");
scanf("%d", &e_velocidade);

if (e_velocidade > 50.0)
 printf("%d maior que 50\n",e_velocidade);

else if (e_velocidade < 50.0)
printf("%d abaixo da velocidade de 50.0\n", e_velocidade);

else {
printf("%d velocidade igual a 50.0\n", e_velocidade);

}

return 0;
}
